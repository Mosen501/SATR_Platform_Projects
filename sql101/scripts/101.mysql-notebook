R(255) UNIQUE,                      -- unique e-mail address\n    office_number  VARCHAR(20)                               -- on-campus office identifier\n) AUTO_INCREMENT = 200000;  -- first generated teacher_id will be 200000\n\n\n\n/* ----------  Subjects table  ---------- */\nCREATE TABLE subjects (\n    subject_id   INT UNSIGNED AUTO_INCREMENT PRIMARY KEY,  -- numeric surrogate key (starts at 300000)\n    subject_name VARCHAR(255) NOT NULL                     -- course title (e.g. “Biology”)\n) AUTO_INCREMENT = 300000;  -- first generated subject_id will be 300000\n\n/* ----------  30 students  ---------- */\nINSERT INTO students\n        (student_name,\n         date_of_birth, gender,\n         date_of_enrollment, student_email,\n         academic_level, track, cumulative_gpa)\nVALUES\n  ('Leen Aldossari',   '2009-08-13', 'F', '2023-09-01', 'leen.aldossari0@example.com', 3, 'humanities', 87),\n  ('Rania Alzahrani',  '2006-02-20', 'F', '2023-09-01', 'rania.alzahrani1@example.com', 3, 'scientific', 80),\n  ('Fatimah Alenzi',   '2009-06-16', 'F', '2023-09-01', 'fatimah.alenzi2@example.com', 5, 'scientific', 89),\n  ('Noura Alqahtani',  '2005-09-16', 'F', '2023-09-01', 'noura.alqahtani3@example.com', 4, 'humanities', 66),\n  ('Nasser Alghamdi',  '2007-07-23', 'M', '2023-09-01', 'nasser.alghamdi4@example.com', 6, 'scientific', 78),\n  ('Nada Alshammari',  '2006-12-28', 'F', '2023-09-01', 'nada.alshammari5@example.com', 1, 'scientific', 94),\n  ('Omar Aldhafeeri',  '2009-08-03', 'M', '2023-09-01', 'omar.aldhafeeri6@example.com', 1, 'humanities', 76),\n  ('Fatimah Alotaibi', '2006-09-11', 'F', '2023-09-01', 'fatimah.alotaibi7@example.com', 5, 'scientific', 75),\n  ('Lina Alharbi',     '2006-10-08', 'F', '2023-09-01', 'lina.alharbi8@example.com', 3, 'scientific', 74),\n  ('Ibrahim Alshehri', '2009-05-16', 'M', '2023-09-01', 'ibrahim.alshehri9@example.com', 1, 'scientific', 86),\n  ('Sultan Alshehri',  '2009-12-30', 'M', '2023-09-01', 'sultan.alshehri10@example.com', 5